{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "api": {
        "runtime": "vercel-node@2.2",
        "memory": 1024
      }
    }
  }